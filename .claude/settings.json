{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "contextFiles": [
    "5-Identity/TELOS.md",
    "5-Identity/AI_RULES.md",
    "5-Identity/CONTEXT.md",
    "6-System/memory/MEMORY.md"
  ],
  "hooks": {
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "python3 ./6-System/hooks/startup.py"
          }
        ]
      }
    ],
    "Stop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "python3 ./6-System/hooks/stop_audit.py"
          }
        ]
      }
    ]
  }
}
